{{~>subtypes name=name}}
{{#if description}}/// {{description}}{{/if}}
{{~#if [$ref]}}
pub type {{camelcase name suffix}} = {{~>data_type required="true"}};
{{~else}}
#[derive(Deserialize, Serialize)]
pub struct {{camelcase name suffix}}
{{~#if type}} {
    {{~#if (eq type "object")}}
        {{~#each properties}}
            {{~#if (eq @key "type")}}
            #[serde(rename = "{{snakecase @key}}")]
            pub _{{snakecase @key}}: {{>data_type name=(camelcase ../name @key)}},
            {{~else}}
            pub {{snakecase @key}}: {{>data_type name=(camelcase ../name @key)}},
            {{~/if}}

        {{~/each}}
    {{~else}}
    pub data: {{~>data_type required="true"}},
    {{~/if}}
}
{{else}}
;
{{~/if}}
{{~/if}}
